{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "jsx-no-lambda": false,
    "prettier": [
      true, {
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}